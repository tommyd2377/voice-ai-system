# Twilio credentials (never commit real values)
TWILIO_ACCOUNT_SID=your_twilio_account_sid
TWILIO_AUTH_TOKEN=your_twilio_auth_token

# OpenAI realtime API key
OPENAI_API_KEY=YOUR_OPENAI_API_KEY_HERE

# OpenAI realtime WebSocket endpoint
OPENAI_REALTIME_ENDPOINT=wss://api.openai.com/v1/realtime?model=gpt-realtime

# Primary Twilio phone number used for call routing
PHONE_RELAY_NUMBER=+15555551234

# WebSocket endpoint for Twilio <Stream>
# Example: wss://your-deployed-domain.com/realtime
TWILIO_STREAM_URL=wss://your-app-domain/realtime

# Local port fallback (ignored in cloud hosting)
PORT=8080

# Firebase backend admin access
FIREBASE_SERVICE_ACCOUNT_JSON={"type":"service_account", ...}